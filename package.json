{
  "name": "tiven-site",
  "version": "1.2.0",
  "private": true,
  "hexo": {
    "version": "3.9.0"
  },
  "scripts": {
    "start": "hexo server --config server.config.yml",
    "dev": "nodemon --watch _config.yml --exec \"hexo clean && hexo g && hexo s \"",
    "build": "hexo generate",
    "clean": "hexo clean",
    "git": "tive git -c tive.git.config.js",
    "push": "tive git -c tive.push.config.js",
    "deploy": "tive git -c tive.deploy.config.js",
    "prezip": "npm run clean",
    "zip": "npm run build && node zip.js",
    "postzip": "node deploy.js",
    "precopy": "npm run del",
    "copy": "node script/sitemap-to-txt.js",
    "del": "node script/del-sitemap.js",
    "prettier": "prettier --write **/*.{js,html,styl,ejs}"
  },
  "dependencies": {
    "hexo": "^3.9.0",
    "hexo-abbrlink": "^2.2.1",
    "hexo-deployer-git": "^0.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-baidu-sitemap": "^0.1.9",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-searchdb": "^1.3.4",
    "hexo-generator-sitemap": "^2.1.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-lazyload-image": "^1.0.10",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-wordcount": "^6.0.1",
    "hexo-yam": "^5.0.0"
  },
  "devDependencies": {
    "adm-zip": "^0.5.5",
    "axios": "^0.21.1",
    "node-ssh": "^12.0.0",
    "rimraf": "^3.0.2",
    "xml2js": "^0.4.23",
    "svgo": "^2.4.0"
  }
}
